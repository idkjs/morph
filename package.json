{
  "name": "@reason-native-web/morph",
  "version": "0.3.1",
  "esy": {
    "build": [
      "refmterr dune build --root=."
    ],
    "install": [
      "esy-installer #{self.target_dir / 'default' / 'morph_test.install'}",
      "esy-installer #{self.target_dir / 'default' / 'morph.install'}"
    ],
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "test": "esy dune build @runtest",
    "fmt": "dune build @fmt --auto-promote --root .",
    "examples": "esy b dune build @docs-examples",
    "examples-path": "esy echo #{self.target_dir}/default/docs",
    "docs": "dune build @doc --only-packages=morph,morph_base,morph_server_base,morph_server_http,morph_server_https",
    "docs-path": "esy echo #{self.target_dir}/default/_doc/_html"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/containers": "*",
    "@opam/dune": "*",
    "@opam/fmt": "*",
    "@opam/graphql-lwt": "0.13.0",
    "@opam/graphql": "0.13.0",
    "@opam/hmap": "*",
    "@opam/logs": "*",
    "@opam/lwt": "^4.0.0",
    "@opam/magic-mime": "*",
    "@opam/uri": "*",
    "@reason-native-web/h1-lwt-unix": "^1.0.0",
    "@reason-native-web/h1-lwt": "^1.0.0",
    "@reason-native-web/h1": "^1.0.0",
    "@reason-native-web/h2-lwt-unix": "^0.5.0",
    "@reason-native-web/h2-lwt": "^0.5.0",
    "@reason-native-web/h2": "^0.5.0",
    "@reason-native-web/lwt_ssl": "^1.1.3001",
    "ocaml": "*"
  },
  "devDependencies": {
    "@opam/mdx": "*",
    "@opam/merlin": "*",
    "@opam/mtime": "*",
    "@opam/ocamlformat": ">=0.13.0",
    "@opam/odoc": "*",
    "@opam/routes": "*",
    "@opam/utop": "*",
    "@reason-native-web/piaf": "^1.0.1002",
    "@reason-native/rely": "*",
    "refmterr": "*"
  }
}
